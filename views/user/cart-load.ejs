         <div class="row" id="cart-list">
            <div class="col-12 col-lg-8">
               <div class="card">
                  <div class="card-header cart_header bg-white">
                     <div class="card-title mb-0">
                        <div class="row">
                           <div class="col-12">
                              <h5 class="mb-0"><i class="fas fa-shopping-cart"></i>&nbsp;&nbsp;Shopping Cart</h5>
                           </div>
                        </div>
                     </div>
                  </div>
                  <% if(data && data.items && data.items.length> 0 ) { %>
                     <div class="card-body cart_body">
                        <div class="row">
                           <% data.items.forEach(function(items, index) { %>
                              <div class="col-12 col-md-6 pr-0">
                                 <div class="d-flex">
                                    <div class="col-xl-4 col-lg-5 col-md-4 col-4 px-0">
                                       <a href="/single-product?id=<%= items.productId._id %>">
                                          <img src="/assets/sharp/<%= items.productId.images[0] %>"
                                             class="fst-image mx-auto d-block img-fluid" alt="product_13">
                                       </a>
                                    </div>
                                    <div class="col-xl-8 col-lg-7 col-md-8 col-8 pr-0">
                                       <h4 class="product-name font-weight-bolder mb-2 mb-sm-3 mb-xl-3 mt-sm-2 mt-md-2">
                                          <a href="#"></a>
                                          <%= items.productId.name %>
                                       </h4>
                                       <div>
                                          <span class="font-weight-bolderer price">₹<%= items.productId.price %></span>
                                          <del class="text-muted">₹1500.00</del>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              <div class="col-12 col-md-6 mt-4">
                                 <div class="d-flex">
                                    <div class="col-md-3 col-sm-4 col-5 text-left text-md-right my-2">
                                       <span class="font-weight-bolder">₹<%= items.productId.price * items.quantity %>
                                       </span>
                                    </div>
                                    <div class="col-md-6 col-sm-4 col-5">
                                       <div class="input-group">
                                          <div class="input-group-prepend">
                                             <button class="btn btn-outline-secondary" type="button" id="sub"
                                                onclick="updateQuantity('<%= items.productId._id %>', -1)">-</button>
                                          </div>
                                          <input type="number" class="form-control border" placeholder=""
                                             value="<%= items.quantity %>" min="1" max="10" disabled>
                                          <div class="input-group-append">
                                             <button class="btn btn-outline-secondary" type="button" id="add"
                                                onclick="updateQuantity('<%= items.productId._id %>', 1)">+</button>
                                          </div>
                                       </div>
                                    </div>
                                    <div class="col-md-3 col-sm-4 col-2">
                                       <button class="btn btn-danger" type="button"
                                          onclick="removeFromCart('<%= items.productId._id %>')">Remove</button>
                                    </div>
                                 </div>
                              </div>
                              <% }) %>
                        </div>
                        <hr>
                        <div class="card-footer">
                           <div class="row">
