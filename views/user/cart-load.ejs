                                    <% var total=0; %>
                                       <% data.items.forEach(function(items) { %>
                                          <% total +=items.productId.price * items.quantity %>
                                             <% }) %>
                                                <span>₹<%= total %></span>
                                 </div>
                              </div>
                              <div class="col-6 col-sm-5 checkout-btn">
                                 <a href="checkout.html" class="btn btn-block f_13 font-weight-bolder">Checkout</a>
                              </div>
                           </div>
                        </div>
                     </div>
                     <% } else { %>
