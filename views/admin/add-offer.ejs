  <script>
    function validateForm() {
      var name = document.getElementById('exampleInputName1').value;
      var amount = document.getElementById('exampleInputEmail3').value;
      var expiryDate = document.getElementById('exampleInputPassword4').value;
  
      var nameError = document.getElementById('nameError');
      var amountError = document.getElementById('amountError');
      var expiryDateError = document.getElementById('expiryDateError');
  
      nameError.innerHTML = "";
      amountError.innerHTML = "";
      expiryDateError.innerHTML = "";
  
      var isValid = true;
  
      if (name === "") {
        nameError.innerHTML = "Name is required";
        isValid = false;
      }
  
      if (amount === "") {
      amountError.innerHTML = "Amount is required";
      isValid = false;
    } else if (amount < 0) {
      amountError.innerHTML = "Amount must be a positive number";
      isValid = false;
    }
  
      if (expiryDate === "") {
        expiryDateError.innerHTML = "Expiry Date is required";
        isValid = false;
      }
  
      return isValid;
    }
  </script>




