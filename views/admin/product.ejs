                        <th class="text text-white"> Price </th>
                        <th class="text-white">Offer </th>
                        <th class="text text-white"> Discount </th>
                        <th class="text text-white"> Edit </th>
                        <th class="text text-white"> Status </th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for (let i=0; i < products.length; i++) {%>
                        <tr>
                          <td class="text text-white">
                            <%= i + 1 %>
                          </td>
                          <td>
                            <img src="assets/multer/<%= products[i].images[0] %>"
                              style="max-width:50px ; max-height:50px ;" alt="product cover image">
                          </td>
                          <td class="text text-white">
                            <%= products[i].name %>
                          </td>
                          <td class="text text-white">
                            <%= products[i].quantity %>
                          </td>
                          <td class="text text-white">
                            <%= products[i].categoryId.name %>
                          </td>
                          <td>
                            <% if (products[i].offer) { %>
                              <s class="text-danger">₹<%= products[i].price %></s>
                              <p class="text-success">
                                <%= products[i].price - products[i].offer.discountAmount %>
                              </p>
                              <% } else if (products[i].categoryDiscount) { %>
                                <s class="text-danger">₹<%= products[i].price %></s>
                                <p class="text-success">
                                  <%= products[i].price - products[i].categoryDiscount %>
                                </p>
                                <% } else { %>
                                  <p class="text-white">₹<%= products[i].price %>
                                  </p>
                                  <% } %>
                          </td>
                          <td class="text-white">
                            <% if (products[i].offer && products[i].offer.discountAmount) { %>
                              <P class="text-info">Product Offer</P>
                              <% } else if (products[i].categoryDiscount) { %>
                                <p class="text-info">Category Offer</p>
                                <% } else { %>
                                  No offer available
                                  <% } %>
                          </td>
                          <td>
                            <% if (!products[i].offer) { %>
                              <button type="button" class="btn btn-success apply-offer" data-bs-toggle="modal"
                                data-bs-target="#exampleModal" onclick="selectProduct('<%= products[i]._id %>')">
                                Apply Offer
                              </button>
                              <% } else { %>
                                <button type="button" class="btn btn-danger"
                                  onclick="removeProductOffer('<%= products[i]._id %>')">
                                  Remove Offer
                                </button>
                                <% } %>
                          </td>
                          <td>
                            <a href="/admin/editproduct?id=<%= products[i]._id %>" class="btn btn-primary">Edit</a>
                          </td>
                          <td class="text-end">
                            <% if (products[i].is_blocked) { %>
                              <a class="Unblock-product" data-product-id="<%= product[i]._id %>"><span
                                  class="btn btn-success border-0 px-4 py-2">Unblock</span></a>
                              <% } else { %>
                                <a class="block-product" data-product-id="<%= products[i]._id %>"><span
                                    class="btn btn-warning border-0 px-4 py-2">Block</span></a>
                                <% } %>
                          </td>
                        </tr>
                        <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
