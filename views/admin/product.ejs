    <script>
      document.querySelectorAll('.Unblock-product, .block-product').forEach(function (element) {
        element.addEventListener('click', function (event) {
          event.preventDefault();
          console.log('sssssAAAAAsssssssssss');
          const productId = element.getAttribute('data-product-id');
          const action = element.classList.contains('block-product') ? 'block' : 'unblock';
          console.log(productId, 'productId');
          fetch(`/admin/blockproduct/${productId}`, {
            method: 'PATCH'
          })
            .then(Response => Response.json())
            .then(data => {
              if (data.block === true) {
                // updating the button and color
                element.querySelector('span').classList.toggle('btn-success');
                element.querySelector('span').classList.toggle('btn-warning');
                element.querySelector('span').innerText = action === 'block' ? 'Unblock' : 'Block';
                //replace button class
                element.classList.remove(action === 'block' ? 'block-user' : 'unblock-user');
                element.classList.add(action === 'block' ? 'unblock-user' : 'block-user');
              }
            })
            .catch((error) => {
              console.log(error);
            });
        });
      });
    </script>

    <!-- Add Offer For Product -->
    <script>
      let productId;

      function selectProduct(selectedProductId) {
        productId = selectedProductId;
        console.log("productId:", productId);
      }

      function applyCoupon(offerId) {
        console.log("offerId", offerId);
        fetch("/admin/product-offer", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify({
            productId: productId,
            offerId: offerId
          }),
        }).then(response => {
          return response.json();
        }).then(data => {
          if (data.success === true) {
            Swal.fire({
              icon: 'success',
              title: 'Offer Added',
              text: 'The offer has been successfully added to the category.',
            });
          }
        })
      }
    </script>

    <!-- Remove Offer From Product  -->
    <script>

      async function removeProductOffer(selectedProductId) {
        try {
          const result = await Swal.fire({
            title: "Are you sure?",
            text: "You won't be able to revert this!",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, delete it!",
          });
          if (result.isConfirmed) {
            console.log("selectProduct:", selectedProductId)
            const response = await fetch("/admin/remove-product-offer", {
              method: "POST",
              headers: {
                "Content-Type": "application/json",
              },
              body: JSON.stringify({
                selectedProductId
              })
            })
            if (response.ok) {
              const data = await response.json();
              if (data.success === true) {
                Swal.fire({
                  icon: 'success',
                  title: 'Offer Deleted',
                  text: 'The offer has been successfully deleted from product.',
                }).then(() => {
                  location.reload();
                });
              }
            }
          }
        } catch (error) {
          console.log(error)
        }
      }
    </script>


